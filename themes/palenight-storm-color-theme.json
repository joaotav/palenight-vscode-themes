  "tokenColors": [
    {
      "name": "Python specific: Decorators",
      "scope": ["meta.function.decorator.python", "entity.name.function.decorator.python"],
      "settings": {
        "foreground": "#C792EA",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python specific: Self/Cls parameter",
      "scope": ["variable.parameter.function.language.special.self.python", "variable.parameter.function.language.special.cls.python"],
      "settings": {
        "foreground": "#F07178",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python specific: Magic methods",
      "scope": ["support.function.magic.python"],
      "settings": {
        "foreground": "#82AAFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python specific: Built-in functions",
      "scope": ["support.function.builtin.python"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Python specific: f-strings",
      "scope": ["meta.fstring.python", "constant.character.format.placeholder.other.python"],
      "settings": {
        "foreground": "#7AEA8D"
      }
    },
    {
      "name": "Bash specific: Variables",
